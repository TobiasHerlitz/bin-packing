{
  "name": "packing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check:eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "storybook": "storybook dev -p 8001 --no-open",
    "build-storybook": "storybook build",
    "test:coverage": "vitest run --coverage",
    "test:tests": "vitest --exclude **/*benchmark.test.ts",
    "test:benchmarks": "vitest run benchmark --run --reporter ./src/tests/metaReporter.ts",
    "test:all": "vitest run",
    "test:ci": "vitest --exclude **/*benchmark.test.ts --run"
  },
  "dependencies": {
    "@react-three/drei": "^9.111.2",
    "@react-three/fiber": "^8.17.5",
    "@react-three/postprocessing": "^2.16.2",
    "@tanstack/react-table": "^8.20.5",
    "docker-names": "^1.2.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-router-dom": "^6.26.1",
    "three": "^0.167.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@eslint/js": "^9.9.0",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/react-vite": "^8.3.5",
    "@types/bun": "^1.1.6",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.10",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.3.3",
    "r3f-perf": "^7.2.2",
    "storybook": "^8.3.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  }
}
